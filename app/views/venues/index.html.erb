<h1>Venues</h1>
<%= link_to 'New Venue', new_venue_path %>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Capacity</th>
    </tr>
  </thead>
  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= link_to venue.name, venue_path(venue) %></td>
        <td><%= venue.address %></td>
        <td><%= venue.capacity %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @venues %>
