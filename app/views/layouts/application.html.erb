<!DOCTYPE html>
<html>
  <head>
    <title>Event Management System</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "tailwind", "inter-font", "data-turbo-track": "reload" %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <main class="prose container mx-auto  px-5">

      <table>
        <thead>
          <tr>
            <th><%= link_to 'Users', users_path %></th>
            <th><%= link_to 'Events', events_path %></th>
            <th><%= link_to 'Categories', categories_path %></th>
            <th><%= link_to 'Tickets', tickets_path %></th>
            <th><%= link_to 'Venues', venues_path %></th>
            <th><%= link_to 'About', about_path %></th>
          </tr>
        </thead>
      </table>
            
      <%= yield %>
    </main>
  </body>
</html>
